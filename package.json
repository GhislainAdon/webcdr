{
  "name": "webcdr",
  "version": "0.0.0",
  "description": "Web Asterisk CDR viewer",
  "main": "server.js",
  "scripts": {
    "watch": "watchify public/js/main.js -d -o public/js/build/bundle.js -v",
    "watchnd": "watchify public/js/main.js -o public/js/build/bundle.js -v",
    "build": "browserify public/js/main.js -o public/js/build/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ivan Poddubny <ivan.poddubny@gmail.com>",
  "license": "Proprietary",
  "dependencies": {
    "express": "~4.4.1",
    "brfs": "~1.1.1",
    "compression": "~1.0.6",
    "mysql": "~2.3.2",
    "knex": "~0.6.22",
    "bookshelf": "~0.7.6",
    "bluebird": "~2.1.1",
    "moment": "~2.7.0",
    "lodash": "~2.4.1",
    "excel-export": "~0.3.11",
    "passport": "~0.2.0",
    "cookie-parser": "~1.1.0",
    "express-session": "~1.2.1",
    "passport-local": "~1.0.0",
    "morgan": "~1.1.1",
    "body-parser": "~1.3.1",
    "glob": "~4.0.2",
    "level": "~0.18.0",
    "connect-leveldb": "frankrowe/connect-leveldb#express4",
    "browserify": "~4.1.11",
    "browserify-shim": "~3.6.0",
    "jquery": "~2.1.1",
    "underscore": "~1.6.0",
    "backbone": "~1.1.2",
    "bcryptjs": "~2.0.1",
    "zip-stream": "~0.4.1",
    "activedirectory": "~0.5.1",
    "ini": "~1.3.0"
  },
  "private": true,
  "browserify": {
    "transform": [
      "brfs",
      "browserify-shim"
    ]
  },
  "browser": {
    "spin.js": "./public/bower_components/spin.js/spin.js",
    "audiojs": "./public/bower_components/audiojs/audiojs/audio.js",
    "moment": "./public/bower_components/moment/min/moment-with-langs.min.js",
    "bootstrap-daterangepicker": "./public/bower_components/bootstrap-daterangepicker/daterangepicker.js",
    "bootstrap-datepicker": "./public/js/vendor/bootstrap-datepicker.js",
    "bootstrap-select": "./public/bower_components/bootstrap-select/bootstrap-select.js",
    "marionette": "./public/bower_components/marionette/lib/backbone.marionette.js",
    "backgrid-paginator": "./public/js/vendor/backgrid-paginator.js",
    "backbone.paginator": "./public/bower_components/backbone.paginator/lib/backbone.paginator.js",
    "backgrid": "./public/bower_components/backgrid/lib/backgrid.js",
    "bootstrap": "./public/bower_components/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "moment": "moment",
    "audiojs": "audiojs",
    "spin.js": "Spinner",
    "bootstrap-daterangepicker": {
      "depends": [
        "moment",
        "jquery:jQuery"
      ]
    },
    "bootstrap-select": {
      "depends": [
        "jquery:jQuery",
        "bootstrap"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
