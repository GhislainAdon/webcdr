    "compression": "~1.0.6",
    "mysql": "~2.3.2",
    "knex": "~0.5.15",
    "bookshelf": "~0.6.12",
    "bluebird": "~2.1.1",
    "moment": "~2.7.0",
    "lodash": "~2.4.1",
    "excel-export": "~0.3.11",
    "passport": "~0.2.0",
    "cookie-parser": "~1.1.0",
    "express-session": "~1.2.1",
    "passport-local": "~1.0.0",
    "morgan": "~1.1.1",
    "body-parser": "~1.3.1",
    "glob": "~4.0.2",
    "level": "~0.18.0",
